{
  "name": "rfft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm --filter rfft-frontend dev",
    "dev:frontend": "pnpm --filter rfft-frontend dev",
    "dev:backend": "pnpm --filter rfft-backend dev",
    "build": "pnpm --filter rfft-frontend build",
    "build:frontend": "pnpm --filter rfft-frontend build",
    "build:backend": "pnpm --filter rfft-backend build",
    "build:check": "pnpm --filter rfft-frontend build:check",
    "lint": "pnpm --filter rfft-frontend lint",
    "lint:fix": "pnpm --filter rfft-frontend lint:fix",
    "format": "pnpm --filter rfft-frontend format",
    "preview": "pnpm --filter rfft-frontend preview",
    "install:all": "pnpm install",
    "clean": "pnpm --filter rfft-frontend clean && pnpm --filter rfft-backend clean"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.131.3",
    "@tanstack/react-router-devtools": "^1.131.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.3.4",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@eslint/js": "^9.32.0",
    "@tanstack/router-plugin": "^1.131.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
